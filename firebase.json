{
	"hosting": {
		"target": "sense-ai",
		"public": "dist",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "**",
				"destination": "/index.html"
			}
		],
		"headers": [
			{
				"source": "**",
				"headers": [
					{
						"key": "X-Frame-Options",
						"value": "SAMEORIGIN"
					},
					{
						"key": "Content-Security-Policy",
						"value": "frame-ancestors 'self' https://web.telegram.org https://*.telegram-apps.com;"
					}
				]
			}
		]
	}
}
